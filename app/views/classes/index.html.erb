<p id="notice"><%= notice %></p>

<h1>Listing Classes</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Course</th>
      <th>Discipline</th>
      <th>Num aluno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classes.each do |classe| %>
      <tr>
        <td><%= classe.codigo %></td>
        <td><%= classe.course.nome %></td>
        <td><%= classe.discipline.nome %></td>
        <td><%= classe.num_aluno %></td>
        <td><%= link_to 'Show', classe %></td>
        <td><%= link_to 'Edit', edit_classe_path(classe) %></td>
        <td><%= link_to 'Destroy', classe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Classe', new_classe_path %>

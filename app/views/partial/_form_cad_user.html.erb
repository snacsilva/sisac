<%= form_for User.new do |f| %>

    <div class="field">
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :data_nascimento %><br>
  <%= f.date_select :data_nascimento, start_year: 1950 %>
</div>
<div class="field">
  <%= f.label :num_identidade %><br>
  <%= f.text_field :num_identidade %>
</div>
<div class="field">
  <%= f.label :cpf %><br>
  <%= f.text_field :cpf %>
</div>
<div class="field">
  <%= f.label :cidade %><br>
  <%= f.text_field :cidade %>
</div>
<div class="field">
  <%= f.label :estado %><br>
  <%= f.text_field :estado %>
</div>
<div class="field">
  <%= f.label :telefone %><br>
  <%= f.text_field :telefone %>
</div>
<div class="field">
  <%= f.label :celular %><br>
  <%= f.text_field :celular %>
</div>
<div class="field">
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</div>
<div class="field">
  <%= f.label :login %><br>
  <%= f.text_field :login %>
</div>
<div class="field">
  <%= f.label :password %><br>
  <%= f.password_field :password %>
</div>
<div class="field">
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %>
</div>


 <div class="actions">
   <%= submit_tag %>
 </div>

<%end%>
